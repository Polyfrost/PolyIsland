  "authors": [
    "Polyfrost"
  ],
  "contact": {
    "homepage": "https://polyfrost.cc",
    "issues": "https://github.com/Polyfrost/Islandtils/issues",
    "sources": "https://github.com/Polyfrost/Islandtils"
  },
  "license": "LGPLv3",
